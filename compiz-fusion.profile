[decoration]
as_command = emerald --replace

[workarounds]
as_convert_urgency = true

[scale]
as_initiate_edge = BottomLeft
s0_spacing = 5
s0_speed = 2.000000
s0_timestep = 1.000000
s0_opacity = 100
s0_multioutput_mode = 1

[scaleaddon]
as_close_button = Button3
as_zoom_button = Disabled

[zoom]
s0_zoom_factor = 1.150000

[core]
as_active_plugins = core;decoration;workarounds;resize;dbus;gnomecompat;crashhandler;mousepoll;fade;move;ezoom;scale;scaleaddon;showdesktop;staticswitcher;
as_edge_delay = 10
as_click_to_focus = false
as_autoraise = false
as_show_desktop_key = Disabled
as_toggle_window_shaded_key = Disabled
s0_hsize = 1
s0_lighting = false
s0_outputs = 1400x1050+0+0;
s0_force_independent_output_painting = true

